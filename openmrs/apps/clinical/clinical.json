   "formConditions.js":"Bahmni.ConceptSet.FormConditions.rules = {'ANC Followup Month' : function (formName, formFieldValues) {var followupMonth = formFieldValues['ANC Followup Month'];if (followupMonth != \"First Visit\") {return {disable: [\"ANC, Gravida\",\"ANC, Parity\",\"ANC, Last Menstrual Period\"]} } else {return {enable: [\"ANC, Gravida\",\"ANC, Parity\",\"ANC, Last Menstrual Period\"]}}},'Diastolic Data' : function (formName, formFieldValues) {var systolic = formFieldValues['Systolic'];var diastolic = formFieldValues['Diastolic'];if (systolic || diastolic) {return {enable: [\"Posture\"]}}else{return {disable: [\"Posture\"]}}},'Systolic Data' : function (formName, formFieldValues) {var systolic = formFieldValues['Systolic'];var diastolic = formFieldValues['Diastolic'];if (systolic || diastolic) {return {enable: [\"Posture\"]}}else{return {disable: [\"Posture\"]}}}};",
